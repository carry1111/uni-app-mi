}
.order .my {
    display: inline-block;
    width: 50%;
}
.order .all {
    display: inline-block;
    width: 50%;
    text-align: right;
    font-size: 26rpx;
}
.order::after {
    content: '';
    position: absolute;
    top: 50%;
    right: 10rpx;
    width: 40rpx;
    height: 40rpx;
    -webkit-transform: translateY(-50%);
        -ms-transform: translateY(-50%);
            transform: translateY(-50%);
