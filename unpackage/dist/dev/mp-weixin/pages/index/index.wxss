.content {
  text-align: center;
  background-color: #f2f2f2;
  overflow-y: scroll;
  overflow-x: hidden;
}
.interval {
  height: 151rpx;
}
.top {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  width: 100%;
  -webkit-box-shadow: 0 4rpx 10rpx 1rpx rgba(0, 0, 0, 0.2);
          box-shadow: 0 4rpx 10rpx 1rpx rgba(0, 0, 0, 0.2);
}
.header {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  background-color: #f2f2f2;
}
.header .icon {
    width: 100rpx;
    height: 86rpx;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 58rpx;
}
.header .search {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
        -ms-flex: 1;
            flex: 1;
    height: 66rpx;
    line-height: 66rpx;
    margin-top: 10rpx;
    padding-left: 10rpx;
    border: 2rpx solid #e5e5e5;
    background-color: #fff;
    text-align: left;
    color: rgba(0, 0, 0, 0.3);
    font-size: 30rpx;
}
.header .search .search-icon {
      display: inline-block;
      width: 36rpx;
      height: 36rpx;
      margin-right: 10rpx;
      vertical-align: middle;
      background: url(data:image/png;base64,) no-repeat center;
      background-size: 36rpx;
}
.header .user {
    width: 100rpx;
    height: 86rpx;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 58rpx;
}
.nav {
  position: relative;
}
.tab {
  background-color: #f2f2f2;
  font-size: 26rpx;
}
.down {
  position: absolute;
  right: 10rpx;
  top: 15rpx;
  z-index: 2;
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-color: #f2f2f2;
  background-size: 40rpx;
  transition: transform .2s linear,-webkit-transform .2s linear;
}
.unfold {
  -webkit-transform: rotate(180deg);
  -ms-transform: rotate(180deg);
      transform: rotate(180deg);
}
.nav-layer {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  padding: 20rpx 20rpx 0 20rpx;
  background-color: #f2f2f2;
  text-align: left;
}
.nav-layer .title {
    text-align: left;
    height: 70rpx;
    font-size: 28rpx;
}
.nav-layer .item {
    display: inline-block;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    width: 21%;
    height: 60rpx;
    line-height: 60rpx;
    margin-right: 3%;
    margin-bottom: 20rpx;
    border: 1rpx solid #e5e5e5;
    border-radius: 8rpx;
    background-color: #fff;
    text-align: center;
    font-size: 24rpx;
}
.nav-layer .selected {
    background-color: #fde0d5;
    border-color: #ff6700;
    color: #ff6700;
}
.backTop {
  position: fixed;
  bottom: 130rpx;
  right: 30rpx;
  z-index: 9999;
}
.popup-mask {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 100;
  background-color: rgba(0, 0, 0, 0.3);
}

