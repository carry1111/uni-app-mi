<view class="content" bind:__l="__l"><block wx:if="{{isEmpty}}"><empty></empty></block><block wx:if="{{!isEmpty}}"><view><view class="list"><block wx:for="{{shopCartList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item"><view data-event-opts="{{[['tap',[['change',[index]]]]]}}" class="{{[(true)?'radio':'',(checkeds[index])?'checked':'',(!checkeds[index])?'unchecked':'']}}" bindtap="__e"></view><image class="img" src="{{item.imageUrl[0]}}"></image><view class="info"><view class="name">{{item.name}}</view><view class="price">{{"售价："+item.price+"元"}}</view><view class="num"><view class="main"><view class="box"><label data-event-opts="{{[['tap',[['changeNum',[-1,index]]]]]}}" class="btn _span" bindtap="__e">-</label><label class="number _span">{{item.num}}</label><label data-event-opts="{{[['tap',[['changeNum',[1,index]]]]]}}" class="btn _span" bindtap="__e">+</label></view></view><view data-event-opts="{{[['tap',[['delProduct',[index]]]]]}}" class="del" bindtap="__e"></view></view></view></view></block></view></view></block><block wx:if="{{isEmpty}}"><tab-bar></tab-bar></block><block wx:if="{{!isEmpty}}"><view><view class="footer"><view class="left"><view>{{"共"+totalNum+"件 金额:"}}</view><view><label class="num _span">{{totalPrice}}</label><label class="_span">元</label></view></view><view data-event-opts="{{[['tap',[['toHome',['$event']]]]]}}" class="center" bindtap="__e">继续购物</view><view data-event-opts="{{[['tap',[['toCheckout',['$event']]]]]}}" class="right" bindtap="__e">去结算</view></view></view></block></view>