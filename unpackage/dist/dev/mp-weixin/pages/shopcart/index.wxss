.content {
  background-color: #f2f2f2;
}
.login {
  background-color: #fff;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 70rpx 0 30rpx;
  position: relative;
}
.login .main {
    display: inline-block;
    width: 82%;
    font-size: 32rpx;
}
.login .aside {
    display: inline-block;
    width: 18%;
    color: rgba(0, 0, 0, 0.6);
    font-size: 26rpx;
    text-align: right;
}
.login::after {
    content: '';
    position: absolute;
    right: 30rpx;
    top: 55%;
    width: 40rpx;
    height: 40rpx;
    -webkit-transform: translateY(-50%);
        -ms-transform: translateY(-50%);
            transform: translateY(-50%);
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 30rpx;
}
.item {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  margin-bottom: 20rpx;
  padding: 20rpx 15rpx;
  background-color: #fff;
}
.radio {
  width: 40rpx;
}
.checked {
  background: url(//s1.mi.com/m/images/m/check_press.png) 50% 50% no-repeat;
  background-size: 40rpx 40rpx;
}
.unchecked {
  background: url(//s1.mi.com/m/images/m/check_normal.png) 50% 50% no-repeat;
  background-size: 40rpx 40rpx;
}
.img {
  display: block;
  border: 1rpx solid #eee;
  width: 180rpx;
  height: 180rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
}
.info {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
      -ms-flex: 1;
          flex: 1;
  padding-left: 10rpx;
}
.info .name {
    width: 300rpx;
    font-size: 27rpx;
    margin-bottom: 8rpx;
}
.info .num {
    font-size: 30rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
}
.info .num .main {
      -webkit-box-flex: 1;
      -webkit-flex: 1;
          -ms-flex: 1;
              flex: 1;
}
.info .num .main .box {
        display: inline-block;
        border: 1rpx solid #f2f2f2;
}
.info .num .number {
      display: inline-block;
      width: 60rpx;
      height: 60rpx;
      text-align: center;
      line-height: 60rpx;
}
.info .num .btn {
      display: inline-block;
      width: 60rpx;
      height: 60rpx;
      font-size: 40rpx;
      text-align: center;
      background-color: #f2f2f2;
}
.info .num .del {
      width: 100rpx;
      background: url(data:image/png;base64,) no-repeat center;
      background-size: 50rpx;
}
.info .price {
    margin-bottom: 10rpx;
    font-size: 24rpx;
    color: #999;
}
