.all {
  height: 100rpx;
}
.guess image {
  width: 100%;
  height: 130rpx;
}
.like ._ul {
  background-color: #fff;
}
.like ._ul ._li {
    display: inline-block;
    width: 49.5%;
    margin-top: 8rpx;
}
.like ._ul ._li image {
      width: 100%;
      height: 360rpx;
}
.like ._ul ._li:nth-child(odd) {
