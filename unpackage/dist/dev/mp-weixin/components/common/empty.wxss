.empty {
  height: 140rpx;
  background-color: #ebebeb;
  text-align: center;
}
.text {
  display: inline-block;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  padding-left: 70rpx;
  background: url(data:image/png;base64,) no-repeat left;
  background-size: 60rpx;
  color: rgba(0, 0, 0, 0.27);
}
.btn {
  display: inline-block;
  border: 1rpx solid rgba(0, 0, 0, 0.15);
