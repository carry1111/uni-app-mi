.content {
  position: relative;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #eee;
  text-align: center;
}
.back {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  left: 0;
  top: 0;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 40rpx;
}
.text {
  font-size: 30rpx;
