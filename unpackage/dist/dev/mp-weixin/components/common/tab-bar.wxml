<view bind:__l="__l"><view class="tab-bar"><view data-event-opts="{{[['tap',[['toHome',['$event']]]]]}}" class="home" bindtap="__e"><view class="{{[(true)?'home-off':'',(tabBarIndex===0)?'home-on':'']}}"></view><label class="{{['_span',(tabBarIndex===0)?'text':'']}}">首页</label></view><view data-event-opts="{{[['tap',[['toClassify',['$event']]]]]}}" class="classify" bindtap="__e"><view class="{{[(true)?'classify-off':'',(tabBarIndex===1)?'classify-on':'']}}"></view><label class="{{['_span',(tabBarIndex===1)?'text':'']}}">分类</label></view><view data-event-opts="{{[['tap',[['toShopcart',['$event']]]]]}}" class="shopcart" bindtap="__e"><view class="{{[(true)?'shopcart-off':'',(tabBarIndex===2)?'shopcart-on':'']}}"></view><label class="{{['_span',(tabBarIndex===2)?'text':'']}}">购物车</label><count class="count" num="{{this.cartList.length}}"></count></view><view data-event-opts="{{[['tap',[['toMy',['$event']]]]]}}" class="my" bindtap="__e"><view class="{{[(true)?'my-off':'',(tabBarIndex===3)?'my-on':'']}}"></view><label class="{{['_span',(tabBarIndex===3)?'text':'']}}">我的</label></view></view><view class="partition"></view></view>