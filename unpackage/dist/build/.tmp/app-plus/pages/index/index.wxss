.content {
  text-align: center;
  background-color: #f2f2f2;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  overflow-y: scroll;
  overflow-x: hidden;
}
.header {
  background-color: #f2f2f2;
}
.header .icon {
    float: left;
    width: 100rpx;
    height: 100rpx;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 50rpx;
}
.header .search {
    float: left;
    width: 540rpx;
    height: 80rpx;
    line-height: 80rpx;
    margin-top: 10rpx;
    padding-left: 10rpx;
    background-color: #fff;
    text-align: left;
    color: rgba(0, 0, 0, 0.3);
}
.header .user {
    float: right;
    width: 100rpx;
    height: 100rpx;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 50rpx;
}
.tab {
  background-color: #f2f2f2;
}
.tab-selected {
  color: #ed5b00;
}
.swiper-item .swiper-img {
  display: inline-block;
  width: 750rpx;
  height: 300rpx;
}
.nav ._li {
  float: left;
  width: 20%;
  background-color: #fff;
}
.nav ._li .img {
    display: inline-block;
    width: 150rpx;
    height: 150rpx;
}
.container {
  position: relative;
  margin: 10rpx 0;
}
.container .left {
    position: absolute;
    left: 0;
    top: 0;
}
.container .left ._img {
      width: 375rpx;
      height: 405rpx;
}
.container .right {
    width: 375rpx;
    margin-left: 380rpx;
}
.container .right ._a {
      display: block;
      width: 370rpx;
      height: 200rpx;
}
.container .right ._a ._img {
        width: 370rpx;
        height: 200rpx;
}
.container .right .top {
      margin-bottom: 5rpx;
}
.banner ._img {
  display: inline-block;
  width: 750rpx;
  height: 300rpx;
}
.banner1 {
  margin: 10rpx 0;
}
.banner1 ._img {
    display: inline-block;
    width: 750rpx;
    height: 400rpx;
}
.mobile {
  margin: 10rpx 0;
  background-color: #fff;
}
.mobile .mobile-ul ._li {
    float: left;
    width: 50%;
}
.mobile .mobile-ul ._li .big {
      display: inline-block;
      width: 375rpx;
      height: 400rpx;
}
.mobile .mobile-ul ._li .info {
      text-align: left;
}
.mobile .mobile-ul ._li .text {
      font-size: 24rpx;
}
.mobile .mobile-foot {
    padding: 20rpx 0;
    border-top: 1rpx solid #ccc;
}
.cate ._li {
  float: left;
  width: 50%;
}
.cate ._li ._img {
    display: inline-block;
    width: 375rpx;
    height: 400rpx;
}
.foot-banner ._img {
  width: 750rpx;
  height: 400rpx;
}
.foot-text {
  padding: 20rpx 0;
